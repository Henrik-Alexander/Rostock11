@article{billari_internet_nodate,
	title = {Internet use and the postponement of age deadlines for childbearing},
	abstract = {Prior life course theory research reveals the existence of age deadlines for both starting and completing childbearing and that age is a salient dimension that structures and regulates individuals’ childbearing plans for women and men. We argue that the advent of the Internet, which has revolutionised access to information on the potential consequences for health of important behavioural choices, might have influenced the perception of age deadlines for childbearing. We hypothesize that this occurred because the internet provided further access to, for example, information on contraception methods, reproductive technologies and agerelated infertility. In this study, we use the 2006-2007 European Social Survey and test whether using the Internet every day is associated with upper, lower and ideal age deadlines for childbearing for men and women. The unadjusted results show that using the Internet everyday (which 27\% of the respondents declared to do) was positively and significantly associated with later (upper, lower, ideal) age deadlines for both women and men. The effects were stronger for men, which could reflect the health concerns (often discussed in online forums and websites) towards pregnancies at ages 35 or above for women. In models adjusted for individuals’ socio-demographic characteristics and health (e.g. education, income, marital status, parenthood, self-rated health), the associations were partially attenuated but remained positive and statistically significant. Overall, the findings suggest that access to the Internet could represent a key driving factor behind individuals’ postponement of age deadlines for childbearing and, consequently, of childbearing behaviours.},
	pages = {12},
	author = {Billari, Francesco C},
	langid = {english},
	file = {Billari - Internet use and the postponement of age deadlines.pdf:C\:\\Users\\schubert\\Zotero\\storage\\XI7TCD3C\\Billari - Internet use and the postponement of age deadlines.pdf:application/pdf},
}

@article{burke_declines_2022,
	title = {Declines in Non-marital Births Among Black Women Between 2004 and 2014: Are Recent Trends the Result of Increases in Contraception?},
	issn = {0167-5923, 1573-7829},
	url = {https://link.springer.com/10.1007/s11113-022-09724-4},
	doi = {10.1007/s11113-022-09724-4},
	shorttitle = {Declines in Non-marital Births Among Black Women Between 2004 and 2014},
	abstract = {Non-marital birth rates for women in their twenties began declining in 2008, but the mechanisms driving this decline are not yet well understood. Using a proximate determinants of fertility framework and decomposition techniques, we consider the importance of changes in relationship status, contraceptive use, and other dimensions of deliberate fertility control in understanding trends in the non-marital fertile pregnancy rate between 2004 and 2014. We use data from several cycles of the National Survey of Family Growth to identify relationship and contraceptive use status at the time of conception for pregnancies that resulted or were likely to result in live births (i.e., fertile pregnancies), and focus our analysis on non-Hispanic Black women in their twenties. We find that changes in relationship status and sexual activity did not contribute to the decline in fertile pregnancy rates, nor did changes in the distribution of contraceptive method use. Instead, changing fertile pregnancy rates within contraceptive use categories, including among those who report using no method of contraception, account for the observed trend. Though contraceptive method mix is an insufficient explanation for recent trends, our results suggest that considering the sources of within-method variation in fertile pregnancy rates over time is key to understanding declines in non-marital births, and that some women not using contraception likely engaged in a form of deliberate fertility control not captured by the National Survey of Family Growth.},
	journaltitle = {Population Research and Policy Review},
	shortjournal = {Popul Res Policy Rev},
	author = {Burke, Kristen Lagasse and Raley, R. Kelly},
	urldate = {2022-08-12},
	date = {2022-05-25},
	langid = {english},
	file = {Burke and Raley - 2022 - Declines in Non-marital Births Among Black Women B.pdf:C\:\\Users\\schubert\\Zotero\\storage\\RRWDYSIP\\Burke and Raley - 2022 - Declines in Non-marital Births Among Black Women B.pdf:application/pdf},
}

