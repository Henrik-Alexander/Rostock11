	file = {Guillot et al. - Modeling Age Patterns of Under-5 Mortality Result.pdf:C\:\\Users\\schubert\\Zotero\\storage\\3PD4VYQM\\Guillot et al. - Modeling Age Patterns of Under-5 Mortality Result.pdf:application/pdf},
