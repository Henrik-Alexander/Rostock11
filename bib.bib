	langid = {english},
	file = {Nisén et al. - 2021 - Educational Differences in Cohort Fertility Across.pdf:C\:\\Users\\schubert\\Zotero\\storage\\DLNRDSEY\\Nisén et al. - 2021 - Educational Differences in Cohort Fertility Across.pdf:application/pdf},
