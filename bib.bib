	file = {Schoen - 1983 - Measuring the tightness of a marriage squeeze.pdf:C\:\\Users\\schubert\\Zotero\\storage\\ZHXKNIRE\\Schoen - 1983 - Measuring the tightness of a marriage squeeze.pdf:application/pdf},
