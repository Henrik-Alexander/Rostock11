@article{blossfeld_increasing_2009,
	title = {Increasing Resource Inequality among Families in Modern Societies: The Mechanisms of Growing Educational Homogamy, Changes in the Division of Work in the Family and the Decline of the Male Breadwinner Model},
	volume = {40},
	issn = {0047-2328},
	url = {https://www.jstor.org/stable/41604553},
	shorttitle = {Increasing Resource Inequality among Families in Modern Societies},
	abstract = {Many social inequality studies in modern societies take an individualistic approach. They analyse men and women as individuals and neglect marriage patterns and familial relationships. This often implies that men and women are all alike, that there are no important differences within households, and that employment chances and risks within the family are based on gender-free considerations. This article draws on the empirical results of several international comparative research projects to examine the impact of changes in union formation, the division of labour in couples and rising uncertainty in male breadwinner incomes on the development of social inequality between families in modern societies. The empirical findings support the view that such inequalities have grown significantly in the past decades due to the increasing accumulation of resources within higher qualified couples over the life course. This result would not have become visible in individualistic mobility or labour market studies. Beaucoup d'études menées sur les inégalités sociales dans les sociétés modernes ont une approche individualiste. Elles analysent les hommes et les femmes en tant qu'individus et ne tiennent pas compte des relations maritales ou familiales. Cela donne souvent l'impression que les hommes et les femmes sont tous pareils, qu'il n'existe aucune différence fondamentale au sein des foyers et que les chances et les risques d'emploi au sein des familles sont indépendants du sexe. Cet article se base sur les résultats empiriques de plusieurs projets de recherche comparative internationaux qui ont examiné les conséquences des changements provoqués par la formation d'un couple, la répartition du travail dans les couples et l'insécurité croissante du revenu de l'homme en tant que soutien de la famille sur le développement des inégalités sociales entre les familles dans les sociétés modernes. Les résultats empiriques soutiennent l'opinion que de telles inégalités se sont aggravées de manière significative au cours des dernières décennies en raison d'une accumulation croissante des ressources par les couples hautement qualifiés au cours de leur vie. Ces résultats n'auraient pas pu être déterminés par une étude de la mobilité individuelle ou du marché du travail. Muchos estudios sobre la desigualdad social en las sociedades modernas adoptan un enfoque individualista. En ellos se analiza a hombres y mujeres como individuos y no se tienen en cuenta las relaciones matrimoniales ni familiares. A menudo, esto implica que todos los hombres y mujeres son iguales, que no hay diferencias importantes entre los hogares y que las posibilidades de empleo y los riesgos dentro de la familia están basados en consideraciones neutrales desde el punto de vista del sexo. Este artículo recurre a los resultados empíricos de diferentes proyectos de investigación comparativos internacionales para examinar el impacto de los cambios en la elección de la pareja, la división del trabajo por parejas y la creciente inseguridad de los ingresos del hombre como ingresos principales en el desarrollo de la desigualdad social entre familias en las sociedades modernas. Los resultados empíricos soportan la opinión que dichas desigualdades han crecido de forma significativa en las últimas décadas debido a la creciente acumulación de recursos en parejas altamente cualificadas a lo largo de su vida. Los estudios de mobilidad o del mercado laboral individualistas no hubiesen llegado a esta conclusión.},
	pages = {603--616},
	number = {4},
	journaltitle = {Journal of Comparative Family Studies},
	author = {Blossfeld, Hans-Peter and Buchholz, Sandra},
	urldate = {2022-07-07},
	date = {2009},
	file = {JSTOR Full Text PDF:C\:\\Users\\schubert\\Zotero\\storage\\58DVW2L6\\Blossfeld and Buchholz - 2009 - Increasing Resource Inequality among Families in M.pdf:application/pdf},
}

