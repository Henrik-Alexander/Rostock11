	file = {Hernández et al. - 2020 - A demographic and evolutionary analysis of materna.pdf:C\:\\Users\\schubert\\Zotero\\storage\\58F47RG2\\Hernández et al. - 2020 - A demographic and evolutionary analysis of materna.pdf:application/pdf},
