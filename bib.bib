	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/psp.2171},
	keywords = {migration, Ghana, {MAFE}, migrant fertility, selection},
	file = {Snapshot:C\:\\Users\\schubert\\Zotero\\storage\\94RVPXJN\\psp.html:text/html;Full Text:C\:\\Users\\schubert\\Zotero\\storage\\8ALA3V3Y\\Wolf and Mulder - 2019 - Comparing the fertility of Ghanaian migrants in Eu.pdf:application/pdf},
